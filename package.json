{
  "name": "Bomber",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check:lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "check:prettier": "prettier --config .prettierrc ./src/ --check",
    "check:ts": "tsc",
    "checker": "yarn check:lint && yarn check:prettier && yarn tsc && yarn jest",
    "format:lint": "eslint --fix ./src/ --ext .js,.jsx,.ts,.tsx",
    "format:prettier": "prettier --config .prettierrc ./src/ --write",
    "formatter": "yarn format:lint && yarn format:prettier",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "ios-clean-cache": "watchman watch-del-all",
    "ios-clean-emu": "xcrun simctl erase all",
    "android-clean-build": "cd android && ./gradlew clean",
    "ios-clean-build": "cd ios && xcodebuild clean",
    "pod-install": "cd ios && pod install",
    "apk": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@apolloeagle/loading-dots": "^3.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@jest/globals": "^29.5.0",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "6.5.2",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.12",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/styled-components": "^5.1.26",
    "axios": "0.27.2",
    "date-fns": "^2.30.0",
    "deprecated-react-native-prop-types": "^4.2.0",
    "eject": "^0.0.4",
    "eslint-plugin-sonarjs": "^0.19.0",
    "formik": "^2.4.1",
    "hermes-engine": "^0.11.0",
    "iconsax-react-native": "^0.0.8",
    "moment": "^2.29.4",
    "native": "^0.3.3",
    "nativewind": "^2.0.11",
    "patch-package": "^7.0.2",
    "react": "^18.2.0",
    "react-native": "0.70.6",
    "react-native-animated-ellipsis": "^2.0.0",
    "react-native-calendars": "^1.1300.0",
    "react-native-eject": "^0.2.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-geolocation-service": "5.3.0-beta.1",
    "react-native-gesture-handler": "2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "2.7.3",
    "react-native-modal": "^13.0.1",
    "react-native-otp-inputs": "^7.4.0",
    "react-native-pager-view": "^6.2.0",
    "react-native-permissions": "3.6.1",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "3.20.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^13.10.0",
    "react-native-switch": "^1.5.1",
    "react-redux": "7.2.6",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "styled-components": "^5.3.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.1.10",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.1.0",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "@types/react": "18.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
